    }

    $final_query = $query . implode(",", $params);

    $stmt = $ease->prepare($final_query);

    $res = $stmt->execute($params_data);

    return $res;
}

/**
 * @param $arr
 * @param \PDO $ease
 * @return bool
 */
function addFixedGroupInDB($arr, $ease)
{
    $truncate_table = "truncate table easecrmfixedgroups";

    $ease->exec($truncate_table);

    $truncate_table = "truncate table easecrmfixedgroupsentities";

    $ease->exec($truncate_table);

    $groups_query = "insert into easecrmfixedgroups(uuid, title, moduleid, created_by, updated_by, created_on, updated_on, issample) VALUES ";
    $entities_query = "insert into easecrmfixedgroupsentities(created_on, groupid, entityid, issample) VALUES ";

    $groups_params = array();
    $groups_params_data = array();
    $entities_params = array();
    $entities_params_data = array();

    $groups_params_str = "(?, ?, ?, ?, ?, ?, ?, ?)";
    $entities_params_str = "(?, ?, ?, ?)";

    foreach($arr as $rowObj)
    {
        $groups_params[] = $groups_params_str;

        $groups_params_data[] = $rowObj['uuid'];
        $groups_params_data[] = $rowObj['title'];
        $groups_params_data[] = $rowObj['moduleid'];
        $groups_params_data[] = $rowObj['created_by'];
        $groups_params_data[] = $rowObj['updated_by'];
        $groups_params_data[] = $rowObj['created_on'];
        $groups_params_data[] = $rowObj['updated_on'];
        $groups_params_data[] = "1";

        foreach($rowObj['entities'] as $entity)
        {
            $entities_params[] = $entities_params_str;

            $entities_params_data[] = $rowObj['created_on'];
            $entities_params_data[] = $rowObj['uuid'];
            $entities_params_data[] = $entity;
            $entities_params_data[] = "1";
        }
    }

    $final_groups_query = $groups_query . implode(",", $groups_params);
    $final_entities_query = $entities_query . implode(",", $entities_params);

    $groups_stmt = $ease->prepare($final_groups_query);
    $entities_stmt = $ease->prepare($final_entities_query);

    $groups_res = $groups_stmt->execute($groups_params_data);
    $entities_res = $entities_stmt->execute($entities_params_data);

    return $groups_res;
}

/**
 * @param $arr
