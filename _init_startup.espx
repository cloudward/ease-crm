 */
function addDynamicGroupInDB($arr, $ease)
{
    $truncate_table = "truncate table easecrmdynamicgroups";

    $ease->exec($truncate_table);

    $truncate_table = "truncate table easecrmdynamicgroupscriteria";

    $ease->exec($truncate_table);

    $groups_query = "insert into easecrmdynamicgroups(created_on, updated_on, uuid, created_by, updated_by, title, moduleid, issample) VALUES ";
    $entities_query = "insert into easecrmdynamicgroupscriteria(created_on, groupid, criteriakey, criteriavalue, operator, operationtype, issample) VALUES ";

    $groups_params = array();
    $groups_params_data = array();
    $entities_params = array();
    $entities_params_data = array();

    $groups_params_str = "(?, ?, ?, ?, ?, ?, ?, ?)";
    $entities_params_str = "(?, ?, ?, ?, ?, ?, ?)";

    foreach($arr as $rowObj)
    {
        $groups_params[] = $groups_params_str;

        $groups_params_data[] = $rowObj['created_on'];
        $groups_params_data[] = $rowObj['updated_on'];
        $groups_params_data[] = $rowObj['uuid'];
        $groups_params_data[] = $rowObj['created_by'];
        $groups_params_data[] = $rowObj['updated_by'];
        $groups_params_data[] = $rowObj['title'];
        $groups_params_data[] = $rowObj['moduleid'];
        $groups_params_data[] = "1";

        foreach($rowObj['criteria'] as $criteria)
        {
            $entities_params[] = $entities_params_str;

            $entities_params_data[] = $criteria['created_on'];
            $entities_params_data[] = $rowObj['uuid'];
            $entities_params_data[] = $criteria['criteriakey'];
            $entities_params_data[] = $criteria['value'];
            $entities_params_data[] = $criteria['operator'];
            $entities_params_data[] = $criteria['operationtype'];
            $entities_params_data[] = "1";
        }
    }

    $final_groups_query = $groups_query . implode(",", $groups_params);
    $final_entities_query = $entities_query . implode(",", $entities_params);

    $groups_stmt = $ease->prepare($final_groups_query);
    $entities_stmt = $ease->prepare($final_entities_query);

    $groups_res = $groups_stmt->execute($groups_params_data);
    $entities_res = $entities_stmt->execute($entities_params_data);

    return $groups_res;
}

function random_time_str()
{
    $hour = mt_rand(0, 24);
    $minutes = mt_rand(0, 59);
    $seconds = mt_rand(0, 59);

    $hour = (string)$hour;
    $minutes = (string)$minutes;
    $seconds = (string)$seconds;

    if(strlen($hour) < 2)
        $hour = "0" . $hour;

    if(strlen($minutes) < 2)
        $minutes = "0" . $minutes;

    if(strlen($hour) < 2)
        $seconds = "0" . $seconds;

    return $hour . ":" . $minutes . ":" . $seconds;
}

function getFormattedDateFromString($timestampStr, $takeRandomTime = true)
{
    if($takeRandomTime)
        $timestampStr .= " " . random_time_str();

    $timestamp = new DateTime($timestampStr);
    return date("Y-m-d H:i:s", $timestamp->getTimestamp());
}

