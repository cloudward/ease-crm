function new_uuid()
{
    return bin2hex(openssl_random_pseudo_bytes(16));
}

/**
 * @param $arr
 * @param $table
 * @param \PDO $ease
 * @return bool
 */
function addArrInDB($arr, $table, $ease)
{
    $truncate_table = "truncate table $table";

    $ease->exec($truncate_table);

    $firstObj = $arr[0];

    $fields = array_keys($firstObj);

    $query = "insert into $table(" . implode(", ", $fields) . ") VALUES ";

    $params = array();
    $params_data = array();

    $params_str = str_repeat("?, ", sizeof($fields));

    $params_str = "(" . substr($params_str, 0, strlen($params_str) - 2) . ")";

    foreach($arr as $rowObj)
    {
        $params[] = $params_str;

        $params_data = array_merge($params_data, array_values($rowObj));
    }

    $final_query = $query . implode(",", $params);

    $stmt = $ease->prepare($final_query);

    $res = $stmt->execute($params_data);

    return $res;
}

